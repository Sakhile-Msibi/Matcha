<% include header %>

<div class="ui segment">
	<h2 class="ui right floated header">Messages</h2>
	<br>
	<hr>
<% matched.forEach(function(element) { %>
	<% var pic = element["profilePic"] %>
	<% var signin = element["matched"] %>
		<div class="ui items" style="margin-left:auto;margin-right:auto;display:block">
			<div class="item oklm">
				<a class="ui tiny image" href="/chat/<%= signin %>">
					<img src="/images/upload_images/<%= pic %>">
				</a>
				<div class="middle aligned content">
					<div class="header" style="color:white">
						<i class="like icon"></i>
						<%= signin %>
						<div class="ui left pointing label" style="margin-left=15%;">
							Click on the photo to start chatting
						</div>
					</div>
				</div>
			</div>
		</div>
<% }) %>
</div>

<% include footer %>
